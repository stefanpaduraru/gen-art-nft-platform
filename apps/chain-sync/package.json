{
  "name": "nft-chain-sync",
  "version": "1.0.0",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stefanpaduraru/nft-backend.git"
  },
  "author": "",
  "license": "ISC",
  "private": true,
  "scripts": {
    "build": "tsc",
    "sync:dev": "NODE_ENV=sync.development nodemon --exec 'TS_NODE_TRANSPILE_ONLY=true TS_NODE_COMPILER=typescript-cached-transpile TS_NODE_PROJECT=tsconfig.json node --inspect -r ts-node/register'  ./src/server.ts",
    "render:dev": "NODE_ENV=render.development nodemon --exec 'TS_NODE_TRANSPILE_ONLY=true TS_NODE_COMPILER=typescript-cached-transpile TS_NODE_PROJECT=tsconfig.json node --inspect -r ts-node/register'  ./src/server.ts",
    "lint": "eslint --ext .tsx,.ts",
    "lint:fix:all": "yarn lint --fix ./src/",
    "preinstall": "yarn global add nodemon",
    "prestart": "yarn run build",
    "start": "nodemon index.ts",
    "start:sync:production": "NODE_ENV=sync.production nodemon --exec 'TS_NODE_TRANSPILE_ONLY=true TS_NODE_COMPILER=typescript-cached-transpile TS_NODE_PROJECT=tsconfig.json node -r ts-node/register' ./src/server.ts",
    "start:render:production": "NODE_ENV=render.production nodemon --exec 'TS_NODE_TRANSPILE_ONLY=true TS_NODE_COMPILER=typescript-cached-transpile TS_NODE_PROJECT=tsconfig.json node -r ts-node/register' ./src/server.ts",
    "typecheck": "tsc --project tsconfig.json --noEmit --pretty false"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/stefanpaduraru/nft-backend/issues"
  },
  "homepage": "https://github.com/stefanpaduraru/nft-backend#readme",
  "dependencies": {
    "@types/node-fetch": "^2.6.1",
    "aws-sdk": "^2.1058.0",
    "axios": "^0.22.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-winston": "^4.2.0",
    "helmet": "^4.6.0",
    "iban": "^0.0.14",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "multer": "^1.4.3",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.15",
    "puppeteer": "^13.5.1",
    "puppeteer-core": "^13.5.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "sharp": "^0.30.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.38",
    "typescript-cached-transpile": "^0.0.6",
    "web3": "^1.6.0",
    "web3-utils": "^1.6.0",
    "webpack": "^5.58.1",
    "winston": "^3.3.3",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@openzeppelin/truffle-upgrades": "^1.9.1",
    "@types/bluebird": "^3.5.36",
    "@types/body-parser": "^1.19.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsdom": "^16.2.13",
    "@types/lodash": "^4.14.175",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.10.1",
    "@types/ramda": "^0.27.45",
    "@types/sharp": "^0.29.5",
    "tslint": "^6.1.3",
    "typescript": "^4.4.3"
  },
  "_moduleAliases": {
    "@root": ".",
    "@abis/*": "abis",
    "@app": "src/app",
    "@config/*": "config",
    "@common/*": "src/common",
    "@src/*": "src",
    "@services/*": "src/common/services"
  }
}
